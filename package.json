{
  "name": "@pact-foundation/pact-node",
  "version": "4.0.2",
  "description": "A wrapper for the Ruby version of Pact to work within Node",
  "main": "./src/pact.js",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "ia32"
  ],
  "engine": {
    "node": ">=0.12"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pact-foundation/pact-node.git"
  },
  "keywords": [
    "pact",
    "node",
    "wrapper",
    "mock",
    "service",
    "provider",
    "verifier"
  ],
  "author": "Michel Boudreau <michelboudreau@gmail.com> (codinghitchhiker.com)",
  "contributors": [
    "Matt Fellows <m@onegeek.com.au> (http://www.onegeek.com.au)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pact-foundation/pact-node/issues"
  },
  "dependencies": {
    "@pact-foundation/pact-mock-service": "0.8.7",
    "@pact-foundation/pact-provider-verifier": "0.0.6",
    "bluebird": "^3.3.5",
    "bunyan": "^1.8.0",
    "bunyan-prettystream": "^0.1.3",
    "check-types": "~6.0.0",
    "commander": "^2.9.0",
    "q": "^1.4.1",
    "request": "^2.69.0",
    "superagent": "^1.8.3",
    "superagent-bluebird-promise": "^3.0.0",
    "underscore": "^1.8.3"
  },
  "optionalDependencies": {
    "@pact-foundation/pact-mock-service-darwin": "0.8.7",
    "@pact-foundation/pact-mock-service-win32": "0.8.7",
    "@pact-foundation/pact-mock-service-linux-x64": "0.8.7",
    "@pact-foundation/pact-mock-service-linux-ia32": "0.8.7",
    "@pact-foundation/pact-provider-verifier-darwin": "0.0.6",
    "@pact-foundation/pact-provider-verifier-linux-ia32": "0.0.6",
    "@pact-foundation/pact-provider-verifier-linux-x64": "0.0.6",
    "@pact-foundation/pact-provider-verifier-win32": "0.0.6"
  },
  "bin": {
    "pact": "./bin/pact-node"
  },
  "devDependencies": {
    "basic-auth": "^1.0.4",
    "body-parser": "^1.15.1",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.3.0",
    "cors": "^2.7.1",
    "cross-env": "^1.0.7",
    "express": "^4.13.4",
    "jscs": "^2.1.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.4.1",
    "rimraf": "^2.4.2",
    "sinon": "^1.15.4"
  },
  "scripts": {
    "clean": "rimraf logs/*",
    "lint": "jscs .",
    "test": "./node_modules/.bin/cross-env DEVELOPMENT=true mocha -R spec ./**/*.spec.js --timeout 15000",
    "watch": "nodemon -x npm run dev",
    "watch:debug": "nodemon --debug -q -w assets/ --ext '.' --exec 'npm run lint'",
    "dev": "npm run clean && npm run lint --force && npm test && ./node_modules/.bin/cross-env DEVELOPMENT=true node .",
    "start": "npm run watch"
  },
  "homepage": "https://github.com/pact-foundation/pact-node#readme"
}
