{
  "name": "@pact-foundation/pact-node",
  "version": "3.0.0",
  "description": "A wrapper for the Ruby version of Pact to work within Node",
  "main": "./src/pact.js",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "ia32"
  ],
  "engine": {
    "node": ">=0.12"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pact-foundation/pact-node.git"
  },
  "keywords": [
    "pact",
    "node",
    "wrapper",
    "mock",
    "service"
  ],
  "author": "Michel Boudreau <michelboudreau@gmail.com> (codinghitchhiker.com)",
  "contributors": [
    "David Stanciu <davidstanciu29@gmail.com> (https://github.com/dstanciu29)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pact-foundation/pact-node/issues"
  },
  "dependencies": {
    "bunyan": "^1.8.0",
    "bunyan-prettystream": "^0.1.3",
    "check-types": "~6.0.0",
    "commander": "^2.9.0",
    "q": "^1.4.1",
    "request": "^2.69.0",
    "underscore": "^1.8.3"
  },
  "optionalDependencies": {
    "@pact-foundation/pact-mock-service-linux-ia32": "0.8.3",
    "@pact-foundation/pact-mock-service-linux-x64": "0.8.3",
    "@pact-foundation/pact-mock-service-win32": "0.8.3",
    "@pact-foundation/pact-mock-service-darwin": "0.8.3"
  },
  "bin": {
    "pact": "./bin/pact-node"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "jscs": "^2.1.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.4.1",
    "rimraf": "^2.4.2",
    "sinon": "^1.15.4"
  },
  "scripts": {
    "postinstall": "node ./scripts/check-dependencies.js",
    "clean": "rimraf logs/*",
    "lint": "jscs .",
    "test": "mocha -R spec ./**/*.spec.js --timeout 15000",
    "watch": "nodemon -x npm run dev",
    "watch:debug": "nodemon --debug -q -w assets/ --ext '.' --exec 'npm run lint'",
    "dev": "npm run clean && npm run lint --force && npm test && node .",
    "start": "npm run watch"
  },
  "homepage": "https://github.com/pact-foundation/pact-node#readme"
}
